{
  "extends": "recommended",
  "rules": {
    "CompileStatic": "off",
    "DuplicateNumberLiteral": "off",
    "MethodParameterTypeRequired": "off",
    "MethodReturnTypeRequired": "off",
    "NoDef": "off",
    "UnnecessaryReturnKeyword": "error",
    "formatting.Indentation": {
      "spacesPerIndentLevel": 4,
      "severity": "info"
    }
  }
}
